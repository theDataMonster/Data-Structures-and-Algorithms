package com;

public class BST {
	
	class Node{
		int key;
		Node left,right;
		
		public Node(int item)
		{
			key=item;
			left=right=null;
		}
	}
	
	Node root;
	
	BST()
	{
		root=null;
	}
	
	void insert(int key)
	{
		root=insertRec(root,key);
	}
	
	Node insertRec(Node root,int key){
		
		if(root==null)
		{
			root=new Node(key);
		}
		
	}
	

}
