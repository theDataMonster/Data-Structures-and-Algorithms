package com;

public class RecursiveBubbleSort {
	
	public static void main(String[] args) {
		
		int arr[]= {1,5,3,100,10,9,2,26,20,12};
		
		bubbleSort(arr);
		
	}
	
	static int[] bubbleSort(int arr[])
	{
		if(arr.length==1)
		{
			return arr; 
		}
		int temp=0;
		
		for(int i=0;i<arr.length;i++)
		{
			temp=arr[i];
			arr[i]=arr[i+1];
			arr[i+1]=temp;
		}
	}

}
